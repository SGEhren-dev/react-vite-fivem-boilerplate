{
	"compilerOptions": {
		"target": "ESNext",
		"lib": [ "DOM", "DOM.Iterable", "ESNext" ],
		"skipDefaultLibCheck": true,
		"sourceMap": true,
		"downlevelIteration": true,
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"noEmit": true,
		"isolatedModules": true,
		"jsx": "react",
		"baseUrl": ".",
		"paths": {
			"MyMod/*": [ "./src/*" ]
		},
	},
	"include": [ "src" ],
	"exclude": [ "node_modules" ]
}